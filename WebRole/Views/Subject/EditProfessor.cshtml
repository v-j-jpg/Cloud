
@{
    ViewBag.Title = "EditProfessor";
}

<h2>EditProfessor</h2>


<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<form action="~/Subject/Edit1" method="post">
    <table class="show">
        <tr>

            <td>ID:</td>
            <td><input type="text" name="RowKey" value="@ViewBag.Professor.RowKey" readonly /> </td>
        </tr>

        <tr>
            <td>Name:</td>
            <td><input type="text" name="Name" value="@ViewBag.Professor.Name" /> </td>
        </tr>
        <tr>
            <td>Lastname:</td>
            <td><input type="text" name="Surname" value="@ViewBag.Professor.Surname" /> </td>
        </tr>

        <tr>
            <td colspan="2"></td>
        </tr>
    </table>
    <input type="submit" value="Edit">


</form>



@Html.ActionLink("List of Professors", "GetProfessor")
